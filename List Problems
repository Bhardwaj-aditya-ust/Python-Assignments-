# Length of The List

def listLength(arr):
    #code here
    return len(arr)

# Sum The List

def listSum(arr):
    #code here
    
    # OR
    
    # return sum(arr)
    
    # OR
    
    # total = 0
    # for i in arr:
    #     total += i
        
    # return total
    
    # OR
    total = sum([i for i in arr])
    return total

# Decrement List Values

def decrementList(arr):
    #code here
    # a1 = [i-1 for i in arr]
    # return a1
    
    # or
    for i in range(len(arr)):
        arr[i] -= 1
    return arr    
   
